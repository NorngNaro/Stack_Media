        <activity android:name=".Activity.Edit_profile_Activity" />
        <activity android:name=".Activity.View_Activity" />
        <activity android:name=".Activity.Flash_screen_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Home_Activity" />
        <activity android:name=".Activity.Sign_up_Activity" />
        <activity android:name=".Activity.Sign_in_Activity" />
        <activity android:name=".Activity.Setting_Activity_new" />
